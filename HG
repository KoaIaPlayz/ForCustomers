	Duration = 3;
})game.StarterGui:SetCore("SendNotification", {

	Title = "Notification";

	Text = "Validating Key...";

	Icon = "rbxassetid://3469494080";

	Duration = 10;
})
wait(2)
if Keytext == "HugoBossEpic" then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/KoaIaPlayz/REMOTEMAIN/main/AFS"))() 
    game.StarterGui:SetCore("SendNotification", {

	Title = "Notification!";

	Text = "Correct Key";

	Icon = "rbxassetid://375691700";

	Duration = 5;

})
wait(0.1)

KeyScreen:Destroy() 

else
    wait(5)
	game.Players.LocalPlayer:Kick("Wrong Key.")
end
end)
if game.Players.LocalPlayer.Name == "KoalaP1ayz" then
    KeyScreen:Destroy()
    wait(0.1)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/KoaIaPlayz/REMOTEMAIN/main/AFS"))() 
        game.StarterGui:SetCore("SendNotification", {

	Title = "Notification!";

	Text = "Hello Owner!";

	Icon = "rbxassetid://375691700";

	Duration = 10;

})
end
