-- Scripts:

local function ZWETQBZ_fake_script() -- AboutKh.LocalScript 
	local script = Instance.new('LocalScript', AboutKh)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.Parent.TabsFrame.AboutKhFrame.Visible = true
		script.Parent.Parent.Parent.TabsFrame.CreditsFrame.Visible = false
		script.Parent.Parent.Parent.TabsFrame.EnterKHub.Visible = false
		script.Parent.Parent.Parent.TabsFrame.WhatKhSupportsFrame.Visible = false
	end)
end
coroutine.wrap(ZWETQBZ_fake_script)()
local function ZFZBG_fake_script() -- WhatKhSupports.LocalScript 
	local script = Instance.new('LocalScript', WhatKhSupports)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.Parent.TabsFrame.AboutKhFrame.Visible = false
		script.Parent.Parent.Parent.TabsFrame.CreditsFrame.Visible = false
		script.Parent.Parent.Parent.TabsFrame.EnterKHub.Visible = false
		script.Parent.Parent.Parent.TabsFrame.WhatKhSupportsFrame.Visible = true
	end)
end
coroutine.wrap(ZFZBG_fake_script)()
local function ZWXFZCI_fake_script() -- Credits.LocalScript 
	local script = Instance.new('LocalScript', Credits)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.Parent.TabsFrame.AboutKhFrame.Visible = false
		script.Parent.Parent.Parent.TabsFrame.CreditsFrame.Visible = true
		script.Parent.Parent.Parent.TabsFrame.EnterKHub.Visible = false
		script.Parent.Parent.Parent.TabsFrame.WhatKhSupportsFrame.Visible = false
	end)
end
coroutine.wrap(ZWXFZCI_fake_script)()
local function FYSXGYH_fake_script() -- EnterKoalaHub.LocalScript 
	local script = Instance.new('LocalScript', EnterKoalaHub)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.Parent.TabsFrame.AboutKhFrame.Visible = false
		script.Parent.Parent.Parent.TabsFrame.CreditsFrame.Visible = false
		script.Parent.Parent.Parent.TabsFrame.EnterKHub.Visible = true
		script.Parent.Parent.Parent.TabsFrame.WhatKhSupportsFrame.Visible = false
	end)
end
coroutine.wrap(FYSXGYH_fake_script)()
local function LDSVD_fake_script() -- RainbowDrag.LocalScript 
	local script = Instance.new('LocalScript', RainbowDrag)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end
end
coroutine.wrap(LDSVD_fake_script)()
